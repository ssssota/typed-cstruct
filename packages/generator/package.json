{
	"name": "@typed-cstruct/generator",
	"version": "0.0.1",
	"main": "index.js",
	"module": "index.mjs",
	"types": "index.d.ts",
	"bin": {
		"tcgen": "bin.js",
		"@typed-cstruct/generator": "bin.js"
	},
	"exports": {
		".": {
			"types": "./index.d.ts",
			"import": "./index.mjs",
			"default": "./index.js"
		}
	},
	"napi": {
		"name": "generator",
		"triples": {
			"additional": [
				"aarch64-apple-darwin",
				"aarch64-unknown-linux-gnu",
				"aarch64-unknown-linux-musl",
				"aarch64-pc-windows-msvc",
				"armv7-unknown-linux-gnueabihf",
				"x86_64-unknown-linux-musl",
				"x86_64-unknown-freebsd",
				"i686-pc-windows-msvc"
			]
		}
	},
	"files": ["index.js", "index.d.ts", "bin.js"],
	"license": "MIT",
	"devDependencies": {
		"@napi-rs/cli": "^2.18.4"
	},
	"engines": {
		"node": ">= 10"
	},
	"scripts": {
		"build": "napi build --platform --release",
		"build:debug": "napi build --platform",
		"prepublishOnly": "napi prepublish -t npm",
		"test": "node --test",
		"version": "napi version",
		"move-binaries": "node ./scripts/move-binaries.mjs"
	}
}
