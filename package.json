{
	"name": "typed-cstruct",
	"version": "0.6.0",
	"description": "Read a binary C struct, Use it as a JavaScript object",
	"type": "module",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"scripts": {
		"test": "vitest",
		"build": "tsc -p tsconfig.lib.json",
		"clean": "node -e 'fs.rm(\"dist\",{recursive:true,force:true},()=>{})'",
		"check": "biome check .",
		"fmt": "biome check --write .",
		"coverage": "vitest run --coverage",
		"prepare": "pnpm clean && pnpm build"
	},
	"keywords": ["c", "struct", "proxy", "reader"],
	"files": ["dist"],
	"author": "TOMIKAWA Sotaro <tommysouta@gmail.com>",
	"license": "MIT",
	"packageManager": "pnpm@9.13.0+sha512.beb9e2a803db336c10c9af682b58ad7181ca0fbd0d4119f2b33d5f2582e96d6c0d93c85b23869295b765170fbdaa92890c0da6ada457415039769edf3c959efe",
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@vitest/coverage-v8": "^2.1.5",
		"typescript": "^5.6.3",
		"vitest": "^2.1.4"
	}
}
